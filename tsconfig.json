{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": ["es6", "dom"],
		"sourceMap": true,
		"rootDir": "src",
		"strictPropertyInitialization": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": true /* enable all strict type-checking options */,
		"resolveJsonModule": true /* enable json imports. The package.json file is used to import the registered commands */
	},
	"exclude": ["node_modules", ".vscode-test", "out"]
}
